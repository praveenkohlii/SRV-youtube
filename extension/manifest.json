{
  "manifest_version": 3,
  "name": "robynSRV",
  "version": "1.0",
  "description": "Blocks the video sections on the YouTube homepage and recommended videos section on the video page.",
  "permissions": ["activeTab"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
